<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <children>
    <ImageView fitHeight="712.5" fitWidth="950.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <image>
        <Image url="@Pub%20Interior.jpg" />
      </image>
    </ImageView>
    <Pane prefHeight="750.0" prefWidth="900.0">
      <children>
        <Pane layoutX="127.0" layoutY="24.0" prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="gambleButton" graphicTextGap="30.0" layoutX="303.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#gamble" prefHeight="89.0" prefWidth="279.0" text="GAMBLE" textAlignment="CENTER">
               <font>
                  <Font name="Athelas Bold" size="41.0" />
               </font>
            </Button>
            <Button fx:id="returnToTown" layoutX="16.0" layoutY="682.0" mnemonicParsing="false" onMouseClicked="#returnToTown" prefHeight="13.0" prefWidth="250.0" style="-fx-border-color: #000000; -fx-border-width: 5;" text="Return to Town" textAlignment="CENTER">
               <font>
                  <Font name="Baskerville Bold" size="21.0" />
               </font>
            </Button>
      </children>
    </Pane>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="ALWAYS" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
  </rowConstraints>
</GridPane>
