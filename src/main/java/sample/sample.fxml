<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="500.0" prefWidth="500.0">
         <children>
            <ImageView fitHeight="507.0" fitWidth="500.0" pickOnBounds="true">
               <image>
                  <Image url="@../../resources/Mule%20Opening.jpg" />
               </image>
            </ImageView>
            <Button id="aboutTeamBut" fx:id="aboutTeamBut" layoutX="366.0" layoutY="426.0" mnemonicParsing="false" onMouseClicked="#aboutTeamClicked" text="About the Team" />
            <Button id="gameConfigBut" fx:id="gameConfigBut" layoutX="193.0" layoutY="309.0" mnemonicParsing="false" onMouseClicked="#gameConfigClicked" style="-fx-background-color: #000000;" text="PLAY!" textFill="#d0cfcf">
               <font>
                  <Font name="System Bold" size="34.0" />
               </font></Button>
            <Button fx:id="exitBut" layoutX="437.0" layoutY="460.0" mnemonicParsing="false" onMouseClicked="#exitGame" text="Exit" />
         </children>
      </Pane>
   </children>
</GridPane>
