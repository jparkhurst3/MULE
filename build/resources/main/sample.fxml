<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="500.0" prefWidth="500.0">
         <children>
            <ImageView fitHeight="507.0" fitWidth="500.0" pickOnBounds="true">
               <image>
                  <Image url="@Mule%20Opening.jpg" />
               </image>
            </ImageView>
            <Button id="aboutTeamBut" fx:id="aboutTeamBut" layoutX="399.0" layoutY="470.0" mnemonicParsing="false" onMouseClicked="#aboutTeamClicked" text="About the Team">
               <font>
                  <Font size="10.0" />
               </font></Button>
            <Button id="gameConfigBut" fx:id="gameConfigBut" layoutX="297.0" layoutY="445.0" mnemonicParsing="false" onMouseClicked="#gameConfigClicked" prefHeight="38.0" prefWidth="87.0" text="PLAY!" textFill="#030000">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font></Button>
            <Button fx:id="exitBut" layoutX="5.0" layoutY="5.0" mnemonicParsing="false" onMouseClicked="#exitGame" prefHeight="26.0" prefWidth="42.0" text="Exit" />
            <Button id="gameConfigBut" fx:id="loadGameBut" layoutX="397.0" layoutY="438.0" mnemonicParsing="false" onMouseClicked="#loadGame" prefHeight="20.0" prefWidth="90.0" text="Load Game">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</GridPane>
