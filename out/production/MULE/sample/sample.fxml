<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="500.0" prefWidth="500.0">
         <children>
            <Label layoutX="82.0" layoutY="119.0" text="Welcome to M.U.L.E.">
               <font>
                  <Font size="34.0" />
               </font>
            </Label>
            <Button id="aboutTeamBut" fx:id="aboutTeamBut" layoutX="190.0" layoutY="287.0" mnemonicParsing="false" onMouseClicked="#aboutTeamClicked" text="About the Team" />
            <Button id="gameConfigBut" fx:id="gameConfigBut" layoutX="179.0" layoutY="250.0" mnemonicParsing="false" onMouseClicked="#gameConfigClicked" text="Game Configuration" />
            <Button fx:id="exitBut" layoutX="230.0" layoutY="320.0" mnemonicParsing="false" onMouseClicked="#exitGame" text="Exit" />
         </children>
      </Pane>
   </children>
